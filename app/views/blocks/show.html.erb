<% case @block.estado %>
<% when 0 #bloque normal %>

	<%= render 'blocknormal', block: @block %>

<% when 1 #bloque en subasta %>

	<%= render 'blocksubasta', block: @block %>

<% when 2 #bloque en hotsale %>

	<%= render 'blockhotsale', block: @block %>

<% end %>

<% parent_layout "residencia" %>
